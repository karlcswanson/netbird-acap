{
  "schemaVersion": "1.8.0",
  "acapPackageConf": {
    "setup": {
      "friendlyName": "netbird",
      "appName": "netbird",
      "vendor": "NetBird GmbH",
      "embeddedSdkVersion": "3.0",
      "vendorUrl": "https://netbird.io/",
      "runMode": "respawnOnFailure",
      "version": "VERSION_NO"
    },
    "configuration": {
      "paramConfig": [
        {
          "name": "SetupKey",
          "default": "",
          "type": "string"
        },
        {
          "name": "ManagementURL",
          "default": "https://api.netbird.io:443",
          "type": "string"
        },
        {
          "name": "EnableLazyConnections",
          "default": "no",
          "type" : "bool:no,yes"
        }
      ]
    }
  }
}
